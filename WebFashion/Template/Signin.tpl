<table cellspacing="0" cellpadding="0" border="0" class="login_box login_table popup_table">
	<tr>
		<td class="popup_shadowtop popuptop_left">
		</td>
		<td class="popup_shadowtop popuptop_right">
		</td>
	</tr>
	<tr>
		<td class="popup_shadow popup_top_left">
			 <div id="dd1" class="select-free">
				<div class="bd">
					<div style="width: auto; height: auto;">
	                        
						<div class="popup_background">
							<div class="" id="login_form_container">
								<div class="new_popup_header clearfix">
									<div id="login_box_popup_header" class="fleft">Tạo tài khoản</div>
									<a class="imgsheet popup_closer" onclick="login_box_close();" id="login_box_popup_closer" href="javascript:void(0)" rel="nofollow"/>
								</div>
								<div class="popup_content clearfix" id="contentlogin">
								<div style="padding-bottom: 7px;" class="clearfix">
								<b>Bạn đã có tài khoản?</b> 
								<a href="javascript:showformLogin()" class="quenmatkhau">Đăng nhập</a>
								</div>
								<div class="login_form_line clearfix">
								    <label for="login_email_field">Họ tên:<span style="color: red;">*</span></label>
								    <input type="text" id="name_field" size="25" name="name_field" value=""/>
								</div>
								<div class="login_form_line clearfix">
									  <label for="login_email_field">
										  Email:<span style="color: red;">*</span>
									  </label>
									  <input type="text" id="email" size="15" name="email"/>
								</div>
								<div class="login_form_line clearfix">
								    <label for="login_email_field">Mật khẩu:<span style="color: red;">*</span></label>
								    <input type="password" id="password_field" size="15" name="password" autocomplete="off"/>
								</div>
								<div class="login_form_line clearfix">
								    <label for="login_email_field">Nhập lại mật khẩu:<span style="color: red;">*</span></label>
								    <input type="password" id="password_verify_field" size="15" name="password_verify" autocomplete="off"/>
								</div>
                <div class="login_form_line clearfix">
                    <label for="login_email_field">Điện thoại:</label>
                    <input type="text" id="phone" size="15" name="phone"/>
                </div>
                <div class="login_form_line clearfix">
                    <label for="login_email_field">Di động:</label>
                    <input type="text" id="mobilephone" size="15" name="mobilephone"/>
                </div>
							
								<div class="bottom clearfix">
								<div style="padding-bottom: 5px; font-size: 13px;" class="cond">

								<div class="cond">
								    <input type="checkbox" onclick="CheckEnable();" name="condUse" id="condUse" style="width: 15px; margin-left: -2px;" checked="checked"/>
								    <span>Đồng ý điều kiện <a href="#">thỏa thuận </a> sử dụng</span>
								</div>
								<div class="bottom_btns clearfix">
								    <div id="login_btn" class="fake_btn_dark_lolite">
									    <a rel="nofollow" onclick="submit_signin_form();return false;"><div>Tạo tài khoản</div></a>
								    </div>

								    <a rel="nofollow" href="javascript:void(0)" onclick="document.getElementById('login_box').style.display='none'; return false;" class="fake_btn_off"><div>Thoát</div></a>      
								</div>
								</div>
								    <input type="hidden" id="hdNumdescript" name="hdNumdescript" value="5"/>
								</div>
							</div>
						</div>
						
					</div>
				</div>
			</div>
       </div>
		</td>
		<td class="popup_shadow popup_top_right">
		</td>
	</tr>
	<tr>
		<td class="popup_shadowbottom popup_bottom_left">
		</td>
		<td class="popup_shadowbottom popup_bottom_right">
		</td>
	</tr>
</table>